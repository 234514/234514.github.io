                        var now=(new Date()).getHours();

                        if(now>0&&now<=5){

                            document.write("夜深了，别看了哦，快点去睡觉吧~");

                        }else if(now>5&&now<=10){

                            document.write("早上好，感谢您一起来就访问我的网站~");

                        }else if(now>10&&now<=14){

                            document.write("中午好，您别忙着看网站了，先去吃饭吧~");

                        }else if(now>14&&now<=18){

                            document.write("下午好，您累了一上午了，先休息一会儿吧~");

                        }else{

                            document.write("晚上好，您一定要记得早点睡觉哦~");

                        }

